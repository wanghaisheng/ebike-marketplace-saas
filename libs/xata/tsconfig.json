{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"]
        },
        "target": "es5",
        "lib": ["esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true
    },
    "include": ["src/scripts/seed.mjs"],
    "exclude": ["node_modules"]
}
